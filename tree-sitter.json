  "grammars": [
    {
      "name": "veryl",
      "camelcase": "Veryl",
      "title": "Veryl",
      "scope": "source.veryl",
      "file-types": ["veryl"],
      "injection-regex": "^veryl$",
      "class-name": "TreeSitterVeryl"
    }
  ],
  "metadata": {
    "version": "0.1.0",
    "license": "MIT",
    "description": "Veryl grammar for tree-sitter",
    "authors": [
      {
        "name": "hota1024",
        "email": "dev@hota1024.com",
        "url": "https://hota.codes"
      }
    ],
    "links": {
      "repository": "https://github.com/hota1024/tree-sitter-veryl",
      "funding": "https://github.com/hota1024/tree-sitter-veryl"
    }
  },
  "bindings": {
    "c": true,
    "go": true,
    "java": false,
    "node": true,
    "python": true,
    "rust": true,
    "swift": true,
    "zig": false
  }
